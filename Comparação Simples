<?php
echo "------------------COMPARAÇÃO---------------------\n";

echo "Digite um número\n";
$num1 = (float) readline();

echo "Digite outro número\n";
$num2 = (float) readline();

if ($num1 >$num2){
    echo $num1 . " é maior que " . $num2;
}
else if ($num1 == $num2){
    echo "Ambos os números possuem o mesmo valor";
}
else {
    echo $num2 . " é maior que " . $num1;
}

?>
